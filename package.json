{
  "name": "lexi.studio",
  "version": "0.7.2",
  "description": "Hi, I'm Lexi, a creative artifical general intelligence system (AGI) ready to work with you on artistic and technical projects.",
  "scripts": {
    "dev": "cross-env NODE_ENV=development nodemon --watch 'server/index.ts' --exec 'ts-node' --project tsconfig.server.json server/index.ts",
    "build:server": "tsc --project tsconfig.server.json",
    "build:next": "next build",
    "build": "yarn build:next && yarn build:server",
    "start": "cross-env NODE_ENV=production node dist/index.js",
    "analyze": "cross-env ANALYZE=true next build",
    "analyze:server": "cross-env BUNDLE_ANALYZE=server next build",
    "analyze:browser": "cross-env BUNDLE_ANALYZE=browser next build",
    "lint": "next lint",
    "add-formation": "yarn add @avsync.live/formation",
    "reformation": "yarn remove @avsync.live/formation && rm -rf ./node_modules/@avsync.live/formation && yarn add-formation",
    "kill": "kill $(lsof -t -i:1618)",
    "kill-ws": "kill $(lsof -t -i:1619)",
    "unused-ts": "ts-unused-exports ./tsconfig.json > unused-ts-exports-report.txt"
  },
  "dependencies": {
    "@avsync.live/formation": "^0.9.123",
    "@extractus/article-extractor": "^7.2.8-rc1",
    "@fortawesome/fontawesome-svg-core": "^6.2.1",
    "@fortawesome/free-brands-svg-icons": "^6.2.1",
    "@fortawesome/free-solid-svg-icons": "^6.2.1",
    "@fortawesome/react-fontawesome": "^0.2.0",
    "@google-cloud/text-to-speech": "^4.1.0",
    "@next/bundle-analyzer": "^13.2.3",
    "@reduxjs/toolkit": "^1.9.2",
    "@types/redux-batched-subscribe": "^0.1.4",
    "@types/redux-logger": "^3.0.9",
    "@waylaidwanderer/chatgpt-api": "^1.22.5",
    "axios": "^1.2.1",
    "body-parser": "^1.20.1",
    "chatgpt": "3.4.1",
    "cookie-parser": "^1.4.6",
    "cookie-session": "^2.0.0",
    "cuid": "^2.1.8",
    "express": "^4.18.2",
    "formik": "^2.2.9",
    "html-to-text": "^9.0.2",
    "markdown-to-text": "^0.1.1",
    "multer": "^1.4.5-lts.1",
    "nanoid": "^4.0.0",
    "net": "^1.0.2",
    "next": "12.2.5",
    "puppeteer": "^19.4.0",
    "puppeteer-core": "^19.4.0",
    "react": "18.2.0",
    "react-div-100vh": "^0.7.0",
    "react-dom": "18.2.0",
    "react-redux": "^8.0.5",
    "react-speech-kit": "^3.0.1",
    "redux": "^4.2.1",
    "redux-batched-subscribe": "^0.1.6",
    "redux-logger": "^3.0.6",
    "socket.io-client": "^4.5.4",
    "spark-md5": "^3.0.2",
    "styled-components": "^5.3.5",
    "tls": "^0.0.1",
    "ts-unused-exports": "^9.0.4",
    "uuid": "^9.0.0",
    "wav-encoder": "^1.3.0",
    "ws": "^8.11.0",
    "youtube-captions-scraper": "^2.0.0",
    "youtube-transcript": "^1.0.4",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@netlify/plugin-nextjs": "^4.19.0",
    "@types/express": "^4.17.14",
    "@types/node": "18.7.8",
    "@types/react": "18.0.17",
    "@types/react-dom": "18.0.6",
    "@types/styled-components": "^5.1.26",
    "@types/ws": "^8.5.3",
    "@zeit/next-css": "^1.0.1",
    "cross-env": "^7.0.3",
    "dotenv": "^16.0.3",
    "eslint": "8.22.0",
    "eslint-config-next": "12.2.5",
    "nodemon": "^2.0.20",
    "pupeteer": "^0.0.1",
    "ts-node": "^10.9.1",
    "typescript": "4.7.4"
  }
}
