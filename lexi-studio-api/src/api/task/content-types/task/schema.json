{
  "kind": "collectionType",
  "collectionName": "tasks",
  "info": {
    "singularName": "task",
    "pluralName": "tasks",
    "displayName": "Task",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string"
    },
    "priority": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::priority.priority"
    },
    "task_status": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::task-status.task-status"
    },
    "attachments": {
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ],
      "type": "media",
      "multiple": true
    },
    "description": {
      "type": "string"
    },
    "assignees": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::person.person"
    },
    "started": {
      "type": "datetime"
    },
    "due": {
      "type": "datetime"
    },
    "completed": {
      "type": "datetime"
    }
  }
}
